    "analyse": "node node_modules/webpack/bin/webpack.js --config=webpack.config.js --env.platform=generic --colors --json | node node_modules/webpack-bundle-size-analyzer/webpack-bundle-size-analyzer",
    "octo": "SET NODE_ENV=development & node node_modules/webpack/bin/webpack.js --config=webpack.config.js --env.platform=generic --colors --progress --watch",
    "chat": "nodemon src/server/server"
  },
